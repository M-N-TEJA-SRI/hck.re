pangram
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
bool pan(string s){
    if(s.length()<26)
    return false;
    else{
        set<char>st;
        int i;
        for(i=0;i<s.length();i++){
            st.insert(s[i]);
        }
        return st.size()==26;
    }
}
int main() {
 string s;
 cin>>s;
 cout<<pan(s);
return 0;
}
output
nicola
0
