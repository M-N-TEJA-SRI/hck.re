
calculate sum of indices which the element is even or odd see which one dominates if both of them are equal then print even odd#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
  int n;
  cin>>n;
  int a[n],i,e=0,o=0;
  for(i=0;i<n;i++){
      cin>>a[i];
      if(a[i]%2==0)
      e+=i;
      else
      o+=i;
  }
if(e==o)
cout<<"Even Odd";
else if(e>o)
cout<<"Even";
else
cout<<"Odd";

    return 0;
}
output
4
1 2 3 4
output
Even
