#include <iostream>
#include<bits/stdc++.h>
using namespace std;
bool isvowel(char ch){
    return ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U';
}
int count(string s){
    int i,c=0;
    for(i=0;i<s.length();i++){
        if(isvowel(s[i])==true)
        c++;
    }
    return c;
}
int main() {
   string s;
   cin>>s;
   cout<<count(s);

    return 0;
}
output
teja
2
